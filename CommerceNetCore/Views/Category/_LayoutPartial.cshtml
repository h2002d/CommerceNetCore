@model List<CommerceNetCore.Models.Category>
@{
    ViewData["Title"] = "_LayoutPartial";
}

<ul class="navbar-nav flex-grow-1">
    <li class="nav-item">
        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
    </li>
    @foreach (var item in Model)
    {
        <li class="nav-item">
            <a class="nav-link text-dark" asp-area="" asp-controller="Category" asp-action="/Index/@item.Id">@item.Name</a>
        </li>
    }
</ul>